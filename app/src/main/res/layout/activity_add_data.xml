<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgpurww"
    tools:context=".AddDataActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="74dp"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="ค้นหา"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/comid"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/jamnun"
        android:layout_width="78dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/serial" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="318dp"
        android:layout_height="81dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/alatsi"
        android:shadowColor="#FDFBFB"
        android:shadowDx="2.0"
        android:shadowDy="2.5"
        android:shadowRadius="10"
        android:text="กรอกข้อมูลสินค้า"
        android:textAlignment="center"
        android:textColor="#E1E3E6"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="20dp"
        android:shadowColor="#000000"
        android:shadowDx="2.9"
        android:shadowDy="2.9"
        android:shadowRadius="1.7"
        android:text="ชื่อแบรนด์"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/band"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comid" />

    <EditText
        android:id="@+id/band"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="58dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ชื่อรุ่น"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/run"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="79dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="99dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/band" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ซีเรียลนัมเบอร์"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/serial"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/serial"
        android:layout_width="209dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="44dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/run" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="123dp"
        android:layout_height="24dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="50dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="จำนวนคงเหลือ"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.162"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="44dp"
        android:layout_height="27dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="7dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ราคา"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/cpuspeed"
        app:layout_constraintEnd_toStartOf="@+id/raka"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/jamnun"
        app:layout_constraintTop_toBottomOf="@+id/serial"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/raka"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/serial" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="35dp"
        android:shadowColor="#000000"
        android:shadowDx="2.0"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ความเร็วซีพียู"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/ram"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/cpuspeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="58dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/jamnun" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="56dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ความจุหน่วยความจำ"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/ram"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/ram"
        android:layout_width="176dp"
        android:layout_height="32dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="77dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/cpuspeed" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="32dp"
        android:shadowColor="#000000"
        android:shadowDx="2.1"
        android:shadowDy="2.0"
        android:shadowRadius="1.3"
        android:text="ความจุฮาร์ดดิสก์"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/disk"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <EditText
        android:id="@+id/disk"
        android:layout_width="179dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="55dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ram" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="102dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="64dp"
        android:text="ต่อไป"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/disk" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="1dp"
        android:shadowColor="#FFFFFF"
        android:shadowDx="2.0"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="ค้นหาไอดี"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/comid"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/comid"
        android:layout_width="170dp"
        android:layout_height="31dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/bg"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="103dp"
        android:layout_height="79dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="43dp"
        app:layout_constraintEnd_toStartOf="@+id/textView27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="139dp"
        android:shadowColor="#000000"
        android:shadowDx="2.0"
        android:shadowDy="2.5"
        android:shadowRadius="1.3"
        android:text="เพิ่มรูปภาพ"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/disk" />


</androidx.constraintlayout.widget.ConstraintLayout>